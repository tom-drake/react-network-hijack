{
  "name": "react-network-hijack",
  "version": "0.1.1",
  "description": "A small higher order component for hijacking all network requests while a component is mounted",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\",\"src/**/story/*\"",
    "test": "exit 0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "release-it",
    "prepublish": "npm run build"
  },
  "author": "Thomas Drake <tom@tom-drake.co.uk>",
  "license": "MIT",
  "dependencies": {
    "fetch-mock": "^7.2.5",
    "xhr-mock": "^2.4.1"
  },
  "peerDependencies": {
    "react": ">= 16.3.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addons": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "babel-loader": "^8.0.4",
    "react": "^16.3.x",
    "react-dom": "^16.3.x",
    "react-json-view": "^1.19.1",
    "release-it": "^9.3.0"
